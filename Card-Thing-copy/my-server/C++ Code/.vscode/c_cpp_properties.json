{
    "version": 4,
    "configurations": [
        {
            "name": "Mac M1",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/homebrew/include",
                "/opt/homebrew/Cellar/mysql/9.3.0/include/mysql"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-clang-arm64",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "/opt/homebrew/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ]
}